<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SalesPersonROVO"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  RowClass="model.readonly.SalesPersonROVORowImpl"
  ComponentClass="model.readonly.SalesPersonROVOImpl">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.readonly.SalesPersonROVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bind_CustId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT A.SUPPORT_TEAM_CD, A.CUST_ID  
  FROM PS_CUST_TEAM A, PS_CUSTOMER B     
  WHERE ( A.SETID = B.SETID     
     AND A.CUST_ID = B.CUST_ID     
         AND A.SETID = 'MASTR'    
 AND A.DEFAULT_FLAG = 'Y'    
AND A.CUST_ID =:bind_CustId )]]></SQLQuery>
  <ViewAttribute
    Name="SupportTeamCd"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPORT_TEAM_CD"
    Expression="SUPPORT_TEAM_CD"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="6"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CustId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="15"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CUST_ID"
    Expression="CUST_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="15"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="CustId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
